{
    "name": "rikmeijer/teach.php",
    "description": "teach support",
    "type": "project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-apcu": "*",
        "ext-imagick": "*",
        "aura/router": "3.1.0",
        "aura/session": "^2.1",
        "auth0/auth0-php": "5.5.*",
        "bacon/bacon-qr-code": "^2",
        "cache/memcached-adapter": "^1.0",
        "doctrine/migrations": "2.0.*",
        "eluceo/ical": "^0.14.0",
        "fideloper/proxy": "^4.0",
        "johngrogg/ics-parser": "^1.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "pulledbits/bootstrap": "~1",
        "pulledbits/view": "^0.7",
        "thecodingmachine/tdbm": "5.0.*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^5.9",
        "laravel/homestead": "^10.2",
        "laravel/ui": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "discard-changes": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "rikmeijer\\Teach\\": "src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
