		"johngrogg/ics-parser" : "^1.0",
		"rikmeijer/activerecord.php": "dev-master",
		"aura/router": "^3.0",
		"guzzlehttp/psr7": "^1.3",
		"aura/session": "^2.1",
		"doctrine/migrations": "^1.5"
	},
	"require-dev" : {
